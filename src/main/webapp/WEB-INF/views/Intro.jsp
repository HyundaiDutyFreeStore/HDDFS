				&& location.href.indexOf("membershipSleepRelease") == -1) {
			if (location.href.indexOf("main") == -1) { // 메인이 아닐때 팝업 뜸
				$(".main_popup2").css("display", "block");
				$(".main_popup2").dialog("open");
			} else if (location.href.indexOf("main") != -1
					&& sessionStorage.getItem("selMainSwiperPos") != 0) {//메인인 경우 인트로 아닐때 팝업뜸
