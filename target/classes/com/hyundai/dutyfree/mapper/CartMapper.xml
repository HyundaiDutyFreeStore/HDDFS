<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.hyundai.dutyfree.mapper.CartMapper">
 	<select id="getCartList"
		resultType="com.hyundai.dutyfree.vo.CartVO">
		select * from cart
	</select>
	
	<insert id="insertCart">
		insert into cart values(seq_id.nextval,#{cartstock},to_date(sysdate,'yy/mm/dd'),#{mid},#{pcode})
	</insert>
	
	<select id="">
		select 
		from cart c and product p and productimg i
		where c.pcode=p.pcode and
		
	</select>
 
 
 </mapper>
  
  